<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://www.mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.template.demo.dao.UserDao">


    <select id="findUserById" parameterType="String" resultType="com.template.demo.model.User">
        SELECT userId,password
        FROM `User` WHERE
        user.userId=#{userId}
    </select>


</mapper>